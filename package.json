{
  "name": "gw2-api",
  "version": "0.1.0",
  "description": "Guild Wars 2 API browser client",
  "repository": {
    "type": "git",
    "url": "https://github.com/SergioMorchon/gw2-api.git"
  },
  "scripts": {
    "mkdir:lib": "mkdir -p lib",
